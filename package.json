{
  "name": "farm-management-backend",
  "version": "1.0.0",
  "description": "Node.js Express backend with TypeScript for Farm Management System",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "create-admin": "ts-node -r tsconfig-paths/register scripts/create-admin.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "farm-management",
    "nodejs",
    "express",
    "typescript",
    "firebase",
    "firestore"
  ],
  "author": "Farm Management Team",
  "license": "MIT",
  "dependencies": {
    "@scalar/express-api-reference": "^0.8.24",
    "@types/compression": "^1.8.1",
    "@types/winston": "^2.4.4",
    "bcryptjs": "^3.0.3",
    "better-auth": "^1.3.34",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "7.3.0",
    "firebase": "^12.5.0",
    "firebase-admin": "^13.6.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "isomorphic-dompurify": "^2.31.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.1",
    "multer": "2.0.2",
    "pdfkit": "^0.17.2",
    "prom-client": "^15.1.3",
    "uuid": "^13.0.0",
    "winston": "^3.18.3"
